        "less-loader": "7.3.0",
        "local-web-server": "5.2.1",
        "style-loader": "2.0.0",
        "testcafe": "2.2.0",
        "ts-loader": "8.4.0",
        "ts-node": "10.9.1",
        "tslint": "6.1.3",
        "typescript": "4.7.4",
        "webpack-dev-server": "4.11.1"
