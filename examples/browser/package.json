        "mongo": "bash mongodb.bash",
        "serve": "ws -p 8888 -d dist/",
        "build": "webpack --config webpack.prod.js && cp ./index.html ./dist/index.html"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/pubkey/event-reduce.git"
    },
    "author": "pubkey",
