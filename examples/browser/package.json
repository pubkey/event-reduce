{
    "name": "event-reduce-examples-browser",
    "version": "1.0.0",
    "scripts": {
        "lint": "tslint --project .",
        "start": "webpack-dev-server --config webpack.dev.js",
        "test:e2e": "testcafe chrome -e test/ --hostname localhost",
